includes:
    - phpstan-baseline.neon

parameters:
    level: 5
    paths:
        - wcfsetup/install/files
    excludePaths:
        analyse:
            - wcfsetup/install/files/lib/system/api/
    scanFiles:
        - constants.php
    ignoreErrors:
